extends ../admin/index

#sub-content
  block sub_content
    h3 Lookups

    form(action='/admin/lookup/create', method='put')

      label(for='name') Name
      input(type='text', name='lookup[name]', id='name')

      input(type='submit', value='Save')

    if lookups
      ul#lookups
        each lookup in lookups
          li: a(href='/admin/lookup/#{lookup.name}') #{lookup.name}

