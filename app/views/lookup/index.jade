extends ../admin/index

block sub_content
  h3 Lookups

  form(action='/lookup/create', method='put')

    label(for='name') Name
    input(type='text', name='lookup[name]', id='name')

    input(type='submit', value='Save')

  if lookups
    ul
      each lookup in lookups
        li: a(href='/lookup/#{lookup.name}') #{lookup.name}

